        box (0 2.0 0) (4.0 5.0 4.5);
		fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
	}

	boxToCell
    {
        box (9.41421 0 0) (20 6.0 1.75);
		fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
	}
	
